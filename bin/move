#!/usr/bin/env ruby

require_relative '../lib/move.rb'

print "Welcome to Tic Tac Toe!"

board = [
  " ",
  " ",
  " ",
  " ",
  " ",
  " ",
  " ",
  " ",
  " "
]

print "Welcome to Tic Tac Toe!"
puts "Where would you like to move?"
user_input = gets.strip


move(board, user_input, value = "X")

display_board(board)
